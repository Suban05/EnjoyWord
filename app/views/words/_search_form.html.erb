<%= search_form_for @q, data: { turbo_frame: 'list' } do |f| %>
    <%= hidden_field_tag :dictionary_id, @dictionary.id %>
    <%= f.search_field :content_or_translation_cont,
        class: "block shadow rounded-md border border-gray-200 outline-none px-3 py-2 mr-2",
        oninput: 'this.form.requestSubmit()',
        autocomplete: 'off',
        autofocus: true
    %>
<% end %>
