<%= turbo_frame_tag list_id do %>
  <div class="w-full">
    <div id="flash">
        <%= render 'shared/flash' %>
    </div>
    <%= render 'shared/header',
        create_button_title_path:,
        create_button_link:,
        new_object:,
        list_title_path:,
        is_empty_list: list.empty?,
        load_words_button_link: local_assigns[:load_words_button_link]
    %>
    <div class="mt-4">
      <ul class="space-y-4 mb-10">
        <%= turbo_frame_tag 'list', class: local_assigns[:list_class] do %>
            <%= render list %>
        <% end %>
      </ul>
    </div>
  </div>
<% end %>
