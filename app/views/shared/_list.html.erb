<%= turbo_frame_tag list_id do %>
  <div class="w-full">
    <div id="flash">
        <%= render 'shared/flash' %>
    </div>
    <% local_assigns[:is_empty_list] = list.empty? %>
    <%= render 'shared/header', local_assigns %>
    <div class="mt-4">
      <ul class="space-y-4 mb-10">
        <%= turbo_frame_tag 'list', class: local_assigns[:list_class] do %>
            <%= render list %>
        <% end %>
      </ul>
    </div>
  </div>
<% end %>
